      "source": [
        "def hypothesis(x,y):\n",
        "  for i, val in enumerate(y):\n",
        "    if val == 'yes':\n",
        "      specific_h = x[i].copy()\n",
        "\n",
        "  for i, val in enumerate(x):\n",
        "    if y[i] == 'yes':\n",
        "      for j in range(len(specific_h)):\n",
        "        if val[j] != specific_h[j]:\n",
        "          specific_h[j] = '?'\n",
        "      else:\n",
        "        pass\n",
        "  return specific_h\n",
        "\n",
        "\n",
        "hypothesis(x,y)\n"
      ],
